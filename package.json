{
  "name": "qpdf-app",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "pnpm run gen:pdfjs:worker && vite",
    "build": "pnpm run gen:pdfjs:worker && vue-tsc --noEmit && vite build",
    "preview": "pnpm run gen:pdfjs:worker && vite preview",
    "tauri": "pnpm run gen:pdfjs:worker && tauri",
    "gen:pdfjs:worker": "cp node_modules/pdfjs-dist/build/pdf.worker.mjs ./public"
  },
  "dependencies": {
    "@bokuweb/zstd-wasm": "^0.0.27",
    "@primeuix/themes": "^1.2.1",
    "@tailwindcss/vite": "^4.1.11",
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-dialog": "~2.3.0",
    "@tauri-apps/plugin-opener": "^2",
    "pdfjs-dist": "^5.3.93",
    "primeicons": "^7.0.0",
    "primevue": "^4.3.6",
    "tailwindcss": "^4.1.11",
    "vue": "^3.5.13",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2",
    "@vitejs/plugin-vue": "^5.2.1",
    "typescript": "~5.6.2",
    "vite": "^6.0.3",
    "vue-tsc": "^2.1.10"
  }
}
